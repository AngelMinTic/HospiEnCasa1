@page
@model HospiEnCasa.App.FrontEnd.Pages.MedicoDeleteModel
@{
}
<h2>¿Seguro deseas eliminar este dato?:</h2>
<div>
<h2>@Model.Medico.Nombre @Model.Medico.Apellidos</h2>
</div>
<div>
    Telefono: @Model.Medico.NumeroTelefono
</div>
<div>
    Genero: @Model.Medico.Genero
</div>
<div>
    Enfermera: @Model.Medico.Especialidad
</div>
<div>
    Codigo: @Model.Medico.Codigo
</div>
<div>
    Historia: @Model.Medico.RegistroRethus
</div>
<form method="post">
    <input type="hidden" asp-for="Medico.Id" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-page="./MedicosList" class="btn btn-primary">Lista de Medicos </a>
</form>